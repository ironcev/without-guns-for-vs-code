{ 
    "name": "without-guns", 
    "displayName": "Without Guns", 
    "description": "Takes your guns from you and teaches you hand-to-hand combat", 
    "version": "0.1.0", 
    "publisher": "ironcev", 
    "engines": { 
        "vscode": "^1.16.0" 
    }, 
    "categories": [ 
        "Other" 
    ], 
    "activationEvents": [ 
        "onCommand:withoutGuns.gunsOff" 
    ], 
    "main": "./out/src/extension", 
    "contributes": { 
        "commands": [{ 
            "command": "withoutGuns.gunsOff", 
            "title": "Guns Off" 
        }] 
    }, 
    "scripts": { 
        "vscode:prepublish": "npm run compile", 
        "compile": "tsc -p ./", 
        "watch": "tsc -watch -p ./", 
        "postinstall": "node ./node_modules/vscode/bin/install", 
        "test": "npm run compile && node ./node_modules/vscode/bin/test" 
    }, 
    "devDependencies": { 
        "typescript": "^2.5.2", 
        "vscode": "^1.1.5", 
        "mocha": "^3.5.0", 
        "@types/node": "^7.0.43", 
        "@types/mocha": "^2.2.42" 
    } 
}